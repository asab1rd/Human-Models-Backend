# Human Paris Modeling Agency CMS - Development Environment

NODE_ENV=development

# App Configuration
HOST=0.0.0.0
PORT=1337
PUBLIC_URL=http://localhost:1337
FRONTEND_URL=http://localhost:3000

# Authentication & Security
APP_KEYS="toBeModified1,toBeModified2,toBeModified3,toBeModified4"
API_TOKEN_SALT=tobemodified
ADMIN_JWT_SECRET=tobemodified
TRANSFER_TOKEN_SALT=tobemodified
JWT_SECRET=tobemodified
ENCRYPTION_KEY=tobemodified

# Database Configuration (SQLite for development)
DATABASE_CLIENT=sqlite
DATABASE_FILENAME=.tmp/data.db

# Upload Configuration (local for development)
# No upload provider needed for local development

# Admin Panel Configuration
ADMIN_URL=/admin
SERVE_ADMIN=true
RATE_LIMIT_MAX=100
RATE_LIMIT_DURATION=60000

# Performance & Logging
LOG_LEVEL=debug
LOG_REQUESTS=true
CRON_ENABLED=true
SERVE_STATIC=true
DATABASE_DEBUG=false

# Security Headers (relaxed for development)
WEBHOOKS_POPULATE_RELATIONS=true
IS_PROXIED=false
SOCKET_ENABLED=false

# Request Configuration
REQUEST_TIMEOUT=300000

# Development specific
DATABASE_FORCE_MIGRATION=false
DATABASE_MIGRATIONS=true
