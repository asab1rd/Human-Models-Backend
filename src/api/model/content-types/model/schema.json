{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model",
    "description": "Fashion models and talents"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "maxLength": 100,
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "name",
      "required": true
    },
    "bio": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gender": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "male",
        "female",
        "non-binary"
      ]
    },
    "nationality": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "city": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "Paris",
        "London",
        "Milan",
        "New York",
        "Los Angeles",
        "Sydney",
        "Tokyo"
      ]
    },
    "board": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "women",
        "men",
        "new-faces",
        "curve",
        "commercial"
      ]
    },
    "isNew": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "isFeatured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "isAvailable": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": true
    },
    "dateAdded": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "profileImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "portfolioImages": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "measurements": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "model.measurements",
      "repeatable": false
    },
    "socials": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "model.socials",
      "repeatable": false
    },
    "compCardPdf": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "workExperience": {
      "type": "component",
      "component": "model.work-item",
      "repeatable": true
    },
    "specialties": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "languages": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
